
<div id="login">
  <center><strong><label>New User Registration</label></strong></center>
  <form action="/register" method="post">
    <label>Username:</label><br>
    <input type="text" placeholder="username" name="user_information[username]"><br>
    <label>Email:</label><br>
    <input type="text" placeholder="email" name="user_information[email]"><br>
    <label>Password:</label><br>
    <input type="password" placeholder="password" name="user_information[password]">
    <input type="submit" value='submit'>
  </form>
</div>
